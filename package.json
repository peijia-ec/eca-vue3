{
  "name": "eca-vue3",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:css": "sass --update src/assets/layout/layout.scss:src/assets/layout/layout.css",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
