    "test:e2e:dev": "start-server-and-test 'vite dev --port 9080' http://localhost:9080 'cypress open --e2e'",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primeuix/themes": "^1.0.0-rc.1",
    "@tailwindcss/vite": "^4.0.8",
    "@unhead/vue": "^2.0.0-alpha.19",
    "dayjs": "^1.11.13",
    "pinia": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.5",
    "tailwindcss": "^4.0.8",
    "tailwindcss-primeui": "^0.5.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0",
    "vuex-map-fields": "^1.4.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@primevue/auto-import-resolver": "^4.2.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.31",
